<view class="container">
  <view>{{text}}</view>
  <view>{{array[0].msg}}</view>
  <view>新字段：{{newField.text}}</view>
  <view>haha</view>
  <view bindtap="viewTap">haha</view>
  <button bindtap="addNewField">添加新数据</button>
  <button bindtap="changeField">更改数据</button>
  <navigator open-type='reLaunch' url='/pages/index/index'>重启本页</navigator>
  <view wx:for="{{array}}">{{item.msg}}</view>
  <block wx:for="{{[1, 2, 3]}}">
    <view> {{index}}: </view>
    <view> {{item}} </view>
  </block>

  <view class="weui-cells__title">带跳转的列表项</view>
  <view class="weui-cells weui-cells_after-title">
      <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
          <view class="weui-cell__bd">cell standard</view>
          <view class="weui-cell__ft weui-cell__ft_in-access"></view>
      </navigator>
      <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
          <view class="weui-cell__bd">cell standard</view>
          <view class="weui-cell__ft weui-cell__ft_in-access"></view>
      </navigator>
  </view>
</view>
